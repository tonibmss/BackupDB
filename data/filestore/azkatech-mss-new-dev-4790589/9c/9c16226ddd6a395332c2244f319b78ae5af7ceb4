return child;},_extractAttributes:function(filter,attrs){filter.isDefault=this.searchDefaults[attrs.name]?true:false;filter.description=attrs.string||attrs.help||attrs.name||attrs.domain||'';if(filter.type==='filter'){if(filter.isDefault){filter.defaultRank=-5;}
