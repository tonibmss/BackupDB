odoo.define('web_studio.report_fields',function(require){"use strict";var fieldRegistry=require('web.field_registry');var relationalFields=require('web.relational_fields');var FieldMany2ManySelection=relationalFields.FieldMany2ManyTags.extend({init:function(parent,name,record,options){this._super.apply(this,arguments);options.quick_create=false;options.can_create=false;var selection=options.attrs.selection;if(typeof selection[0]==='string'){selection=_.map(selection,function(s){return[s,s];});}
